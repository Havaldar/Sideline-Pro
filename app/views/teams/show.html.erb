<h1><%=@team.name%></h1>
<%=link_to 'Join', memberships_path(:team_id => @team.id), method: :post%>
<ul>
<%@team.users.each do |user|%>
	<li><%= link_to user.name, user_path(user)%></li>
<%end%>
<%= render :partial => 'games/game_form', :locals => {:game => @game, :teams => @teams, :opponent_team => @team.id} %>
</ul>